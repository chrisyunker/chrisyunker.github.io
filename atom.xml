---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Chris Yunker</title>
 <link href="http://yunker.io/atom.xml" rel="self"/>
 <link href="http://yunker.io/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://yunker.io/</id>
 <author>
   <name>Chris Yunker</name>
   <email>chris@yunker.io</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://yunker.io{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://yunker.io{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
